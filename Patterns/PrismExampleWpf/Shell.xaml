<Window x:Class="PrismExampleWpf.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:inf="clr-namespace:Infrastructure;assembly=Infrastructure"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:local="clr-namespace:PrismExampleWpf"
        mc:Ignorable="d"
        Title="Shell" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ContentControl Grid.Column="0" prism:RegionManager.RegionName="{x:Static inf:RegionNames.Left}"/>
        <ContentControl Grid.Column="1" prism:RegionManager.RegionName="{x:Static inf:RegionNames.Right}"/>
    </Grid>
</Window>
